{ "name": "New Tab",
  "version": "0.1",
  "manifest_version": 2, 
  "description": "Opens Tab!",
  "browser_action": {
   "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.facebook.com/*"],
      "js": ["redirect.js"]
    }
  ],
  "background":{
   "scripts": ["redirect.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation"
  ]
}
